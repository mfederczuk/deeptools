{
	"name": "@mfederczuk/deeptools",
	"version": "1.1.1",
	"description": "A set of utility functions that recursively operate on objects",
	"keywords": [
		"deep",
		"copy",
		"freeze"
	],
	"homepage": "https://github.com/mfederczuk/deeptools#readme",
	"bugs": {
		"url": "https://github.com/mfederczuk/deeptools/issues",
		"email": "federczuk.michael@protonmail.com"
	},
	"license": "GPL-3.0-or-later",
	"author": "Michael Federczuk <federczuk.michael@protonmail.com>",
	"contributors": [
		"Michael Federczuk <federczuk.michael@protonmail.com>"
	],
	"files": [
		"COPYING.txt",
		"LICENSE.txt",
		"LICENSES",
		"out",
		"types"
	],
	"main": "out/index.js",
	"types": "types/index.d.ts",
	"directories": {
		"lib": "out"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/mfederczuk/deeptools.git"
	},
	"scripts": {
		"lint": "npx --package=eslint -- eslint src test",

		"build": "npx --package=typescript -- tsc",
		"build-watch": "npx --package=typescript -- tsc --watch",
		"clean": "rm -rf out types",
		"rebuild": "npm run clean && npm run build --",

		"uglify": "find out -type f -name '*.js' -exec npx --package=uglifyjs -- uglifyjs -o '{}' -- '{}' \\;",

		"test": "npx --package=mocha mocha && npm run testDeepFreeze",
		"testDeepFreeze": "npx --package=ts-node -- ts-node test/deepFreeze.ts",
		"testSafeCopy": "npm run testDeepCopy && npm run testDeepFreeze",

		"update-dependencies": "rm -rf types/ out/ node_modules/ package-lock.json yarn.lock && npm update --save && npx --package=npm-check-updates ncu --upgrade && npm install",
		"version": "./update-version-from-package",

		"prepare": "npm run clean && npm run test && npm run build && npm run uglify"
	},
	"devDependencies": {
		"@types/mocha": "^10.0.1",
		"@types/node": "^18.11.14",
		"@typescript-eslint/eslint-plugin": "^5.46.1",
		"@typescript-eslint/parser": "^5.46.1",
		"eslint": "^8.29.0",
		"mocha": "^10.2.0",
		"npm-check-updates": "^16.5.6",
		"ts-node": "^10.9.1",
		"typescript": "^4.9.4",
		"uglify-js": "^3.17.4"
	}
}
